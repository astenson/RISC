#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1198b50 .scope module, "tb_instructMem" "tb_instructMem" 2 4;
 .timescale 0 0;
v0x11ac940_0 .var "addr", 3 0;
v0x11ac9f0_0 .net "clk", 0 0, v0x11ac870_0; 1 drivers
v0x11acac0_0 .net "instruction", 31 0, v0x11ac700_0; 1 drivers
S_0x11ac780 .scope module, "c" "clock" 2 9, 3 1, S_0x1198b50;
 .timescale 0 0;
v0x11ac870_0 .var "clk", 0 0;
S_0x1198c40 .scope module, "uut" "instructMem" 2 10, 4 1, S_0x1198b50;
 .timescale 0 0;
v0x1176390_0 .net "addr", 3 0, v0x11ac940_0; 1 drivers
v0x11ac510_0 .alias "clk", 0 0, v0x11ac9f0_0;
v0x11ac5b0_0 .alias "instruction", 31 0, v0x11acac0_0;
v0x11ac650 .array "instructions", 71 0, 7 0;
v0x11ac700_0 .var "temp_instruct", 31 0;
E_0x11774b0 .event posedge, v0x11ac510_0;
    .scope S_0x11ac780;
T_0 ;
    %set/v v0x11ac870_0, 0, 1;
T_0.0 ;
    %delay 10, 0;
    %load/v 8, v0x11ac870_0, 1;
    %inv 8, 1;
    %set/v v0x11ac870_0, 8, 1;
    %jmp T_0.0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1198c40;
T_1 ;
    %vpi_call 4 8 "$readmemb", "instructions.txt", v0x11ac650;
    %end;
    .thread T_1;
    .scope S_0x1198c40;
T_2 ;
    %wait E_0x11774b0;
    %ix/getv 3, v0x1176390_0;
    %load/av 8, v0x11ac650, 8;
    %ix/load 0, 8, 0;
    %ix/load 1, 24, 0;
    %assign/v0/x1 v0x11ac700_0, 0, 8;
    %ix/load 0, 1, 0;
    %load/vp0 16, v0x1176390_0, 32;
    %ix/get 3, 16, 32;
    %load/av 8, v0x11ac650, 8;
    %ix/load 0, 8, 0;
    %ix/load 1, 16, 0;
    %assign/v0/x1 v0x11ac700_0, 0, 8;
    %ix/load 0, 2, 0;
    %load/vp0 16, v0x1176390_0, 32;
    %ix/get 3, 16, 32;
    %load/av 8, v0x11ac650, 8;
    %ix/load 0, 8, 0;
    %ix/load 1, 8, 0;
    %assign/v0/x1 v0x11ac700_0, 0, 8;
    %ix/load 0, 3, 0;
    %load/vp0 16, v0x1176390_0, 32;
    %ix/get 3, 16, 32;
    %load/av 8, v0x11ac650, 8;
    %ix/load 0, 8, 0;
    %ix/load 1, 0, 0;
    %assign/v0/x1 v0x11ac700_0, 0, 8;
    %jmp T_2;
    .thread T_2;
    .scope S_0x1198b50;
T_3 ;
    %delay 50, 0;
    %movi 8, 4, 4;
    %set/v v0x11ac940_0, 8, 4;
    %end;
    .thread T_3;
    .scope S_0x1198b50;
T_4 ;
    %delay 50, 0;
    %vpi_call 2 16 "$monitor", "addr=%b : instruction=%b", v0x11ac940_0, v0x11acac0_0;
    %delay 50, 0;
    %vpi_call 2 17 "$finish";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "tb_instructMem.v";
    "./clock.v";
    "./instructMem.v";
