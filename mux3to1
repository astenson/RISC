#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x231ab80 .scope module, "tb_mux3to1" "tb_mux3to1" 2 16;
 .timescale 0 0;
v0x232b9f0_0 .var "A", 15 0;
v0x232bac0_0 .var "B", 15 0;
v0x232bb70_0 .var "C", 15 0;
v0x232bc20_0 .net "result", 15 0, v0x232b8a0_0; 1 drivers
v0x232bd00_0 .var "select", 1 0;
S_0x231ac70 .scope module, "uut" "mux3to1" 2 21, 2 1, S_0x231ab80;
 .timescale 0 0;
v0x22f8100_0 .net "i1", 15 0, v0x232b9f0_0; 1 drivers
v0x232b760_0 .net "i2", 15 0, v0x232bac0_0; 1 drivers
v0x232b800_0 .net "i3", 15 0, v0x232bb70_0; 1 drivers
v0x232b8a0_0 .var "o1", 15 0;
v0x232b950_0 .net "sel", 1 0, v0x232bd00_0; 1 drivers
E_0x2319410 .event edge, v0x232b800_0, v0x232b760_0, v0x22f8100_0, v0x232b950_0;
    .scope S_0x231ac70;
T_0 ;
    %wait E_0x2319410;
    %load/v 8, v0x232b950_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_0.2, 6;
    %jmp T_0.3;
T_0.0 ;
    %load/v 8, v0x22f8100_0, 16;
    %set/v v0x232b8a0_0, 8, 16;
    %jmp T_0.3;
T_0.1 ;
    %load/v 8, v0x232b760_0, 16;
    %set/v v0x232b8a0_0, 8, 16;
    %jmp T_0.3;
T_0.2 ;
    %load/v 8, v0x232b800_0, 16;
    %set/v v0x232b8a0_0, 8, 16;
    %jmp T_0.3;
T_0.3 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x231ab80;
T_1 ;
    %set/v v0x232b9f0_0, 0, 16;
    %movi 8, 1, 16;
    %set/v v0x232bac0_0, 8, 16;
    %movi 8, 2, 16;
    %set/v v0x232bb70_0, 8, 16;
    %set/v v0x232bd00_0, 0, 2;
    %delay 10, 0;
    %movi 8, 1, 2;
    %set/v v0x232bd00_0, 8, 2;
    %delay 10, 0;
    %movi 8, 2, 2;
    %set/v v0x232bd00_0, 8, 2;
    %end;
    .thread T_1;
    .scope S_0x231ab80;
T_2 ;
    %vpi_call 2 33 "$monitor", "time=%d", $time, "select=%b result=%d", v0x232bd00_0, v0x232bc20_0;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "mux3to1.v";
